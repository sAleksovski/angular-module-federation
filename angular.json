            "tsConfig": "projects/ui-ang/tsconfig.spec.json",
            "karmaConfig": "projects/ui-ang/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ui-ang/tsconfig.lib.json",
              "projects/ui-ang/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
