{
  "name": "angular-module-federation",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:check": "prettier --list-different \"{src,projects}/**/*.ts\"",
    "format": "prettier --write \"{src,projects}/**/*.ts\"",
    "build:all": "concurrently \"npm run build:shell\" \"npm run build:contacts-list\" \"npm run build:contacts-details\" \"npm run build:newsfeed\"",
    "serve:all": "concurrently \"npm run serve:shell\" \"npm run serve:contacts-list\" \"npm run serve:contacts-details\" \"npm run serve:newsfeed\"",
    "build:shell": "webpack --config src/webpack.config.js",
    "serve:shell": "http-server -p 5000 dist/angular-module-federation",
    "build:contacts-list": "webpack --config projects/apps/contacts-list/webpack.config.js",
    "serve:contacts-list": "http-server -p 5001 dist/contacts-list",
    "build:contacts-details": "webpack --config projects/apps/contacts-details/webpack.config.js",
    "serve:contacts-details": "http-server -p 5002 dist/contacts-details",
    "build:newsfeed": "webpack --config projects/apps/newsfeed/webpack.config.js",
    "serve:newsfeed": "http-server -p 5003 dist/newsfeed"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.0",
    "@angular/common": "~11.0.0",
    "@angular/compiler": "~11.0.0",
    "@angular/core": "~11.0.0",
    "@angular/forms": "~11.0.0",
    "@angular/platform-browser": "~11.0.0",
    "@angular/platform-browser-dynamic": "~11.0.0",
    "@angular/router": "~11.0.0",
    "@nd/core": "^0.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "resolutions": {
    "webpack": "^5.4.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.1",
    "@angular/cli": "~11.0.1",
    "@angular/compiler-cli": "~11.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^5.3.0",
    "html-webpack-plugin": "^4.5.0",
    "http-server": "^0.12.3",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.2",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webpack-cli": "^4.2.0"
  }
}
